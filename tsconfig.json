  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
